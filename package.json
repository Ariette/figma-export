{
  "name": "figma-export",
  "private": true,
  "license": "MIT",
  "description": "Export tool for Figma",
  "scripts": {
    "lint": "eslint . --ignore-pattern=node_modules",
    "test": "mocha test--registers.js test--packages.js",
    "coverage": "nyc --reporter=lcov --reporter=text npm test -- --reporter=dot",
    "coverage:watch": "npx nodemon --exec npm run coverage",
    "lerna:bootstrap": "lerna bootstrap --force-local",
    "lerna:version": "lerna version",
    "lerna:publish": "lerna publish from-git --require-scripts"
  },
  "author": "Marco Montalbano <me@marcomontalbano.com>",
  "nyc": {
    "all": false,
    "include": [
      "**/*.js"
    ]
  },
  "devDependencies": {
    "chai": "~4.2.0",
    "eslint": "~6.4.0",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-plugin-chai-friendly": "~0.4.1",
    "eslint-plugin-import": "~2.18.2",
    "lerna": "^3.15.0",
    "mocha": "~6.2.0",
    "nock": "~11.3.5",
    "nyc": "~14.1.1",
    "sinon": "~7.5.0",
    "sinon-chai": "~3.3.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">= 8.3 <= 12.x",
    "npm": ">= 5.3 <= 6.x"
  }
}
